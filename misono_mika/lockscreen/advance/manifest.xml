                    </Pressed>
                    <!-- 关闭按钮 -->
                    <Image x="#w_ri7mss-66" y="#h_ri7mss/2" align="center" alignV="center" src="notification_src/vzhc7e8c_close.png"/>
                    <!-- 通知图标 -->
                    <Image name="icon_ri7mss" x="#h_ri7mss/2" y="#h_ri7mss/2" w="130" h="130" align="center" alignV="center"/>
                    <Text name="title_ri7mss" x="#h_ri7mss" y="#h_ri7mss/2-28" w="int(#w_ri7mss-#h_ri7mss-132)-(180-(70*#time_format))" h="48" alignV="center" size="42" color="#ffee68bd" marqueeSpeed="30"/>
                    <Text name="content_ri7mss" x="#h_ri7mss" y="#h_ri7mss/2+30" w="int(#w_ri7mss-#h_ri7mss-132)" h="42" alignV="center" size="36" color="#ffee68bd" marqueeSpeed="30" alpha="128"/>
                    <Text name="time_ri7mss" x="#w_ri7mss-132" y="#h_ri7mss/2-28" align="right" alignV="center" size="36" color="#ffee68bd" alpha="128"/>
                    <Triggers>
                        <Trigger action="down">
                            <VariableCommand name="notice_down" expression="1"/>
                        </Trigger>
                        <Trigger action="up">
                            <IntentCommand action="com.miui.app.ExtraStatusBarManager.action_remove_keyguard_notification" broadcast="true">
                                <Extra name="com.miui.app.ExtraStatusBarManager.extra_notification_key" type="int" expression="#notification_list_ri7mss.key"/>
                                <!-- expression 0 通知消失 1 打开通知 -->
                                <!-- 132 == 66 * 2 依据 关闭按钮 的位置 -->
                                <Extra name="com.miui.app.ExtraStatusBarManager.extra_notification_click" type="int" expression="#touch_begin_x{20+1024-132*#scaleX_ri7mss"/>
                            </IntentCommand>
                        </Trigger>
                    </Triggers>
                </Button>
            </Item>
            <AttrDataBinders>
                <AttrDataBinder target="icon_ri7mss" attr="bitmap" data="icon"/>
                <AttrDataBinder target="title_ri7mss" attr="text" data="title"/>
                <AttrDataBinder target="content_ri7mss" attr="text" data="content"/>
                <AttrDataBinder target="time_ri7mss" attr="text" data="time"/>
            </AttrDataBinders>
        </List>
    </Group>
