<?xml version="1.0" encoding="utf-8"?>
<Lockscreen version="2" frameRate="60" displayDesktop="false" screenWidth="1080" compiler="true">
    <ExternalCommands>
        <Trigger action="resume">
            <!-- 从负一屏或相机界面后界面退出后图标界面复位 -->
            <VariableCommand name="move_x1_hfew9b" expression="0"/>
            <VariableCommand name="move_x2_hfew9b" expression="0"/>
            <AnimationCommand target="move_go_hfew9b" command="play"/>
            <AnimationCommand target="unlock_back_hfew9b" command="play(0,0)"/>
        </Trigger>
        <Trigger action="pause">
            <AnimationCommand target="unlock_back_hfew9b" command="play(0,0)"/>
        </Trigger>
        <Trigger action="resume">
            <AnimationCommand target="panel_framerate_controller_0qgrgr" command="play"/>
            <AnimationCommand target="MusicAni_0qgrgr" command="play(0,0)" condition="#music_control_0qgrgr.music_state==0"/>
            <MultiCommand condition="#music_control_0qgrgr.music_state==1">
                <AnimationCommand target="MusicAni_0qgrgr" command="play(0,700)"/>
                <AnimationCommand target="albumCoverAni_0qgrgr" command="play(0,600)"/>
                <VariableCommand name="resumeMusicButtonVi_0qgrgr" expression="1"/>
                <VariableCommand name="resumeMusicButtonVi_0qgrgr" expression="0" delay="600"/>
            </MultiCommand>
            <VariableCommand name="autoShowVi_0qgrgr" expression="1" condition="#musicAutoShow_0qgrgr==1"/>
            <VariableCommand name="autoShowVi_0qgrgr" expression="0" delay="700" condition="#musicAutoShow_0qgrgr!=1"/>
        </Trigger>
        <Trigger action="pause">
            <AnimationCommand target="MusicAni_0qgrgr" command="play(0,0)"/>
            <AnimationCommand target="albumCoverAni_0qgrgr" command="play(0,0)"/>
            <VariableCommand name="autoShowVi_0qgrgr" expression="1" condition="#musicAutoShow_0qgrgr==1"/>
            <VariableCommand name="autoShowVi_0qgrgr" expression="0" condition="#musicAutoShow_0qgrgr!=1"/>
        </Trigger>
    </ExternalCommands>
    <VariableBinders>
        <ContentProviderBinder name="data_ri7mss" uri="content://keyguard.notification/notifications" columns="icon,title,content,time,info,subtext,key" countName="hasnotifications">
            <List name="notification_list_ri7mss"/>
        </ContentProviderBinder>
    </VariableBinders>
    <Group name="blok_4krk4d3j" visibility="1">
        <!-- 系统静态壁纸	SystemWallpaper -->
        <!-- 全屏适配 -->
        <Wallpaper w="#screen_width" h="#screen_height" alpha="255"/>
    </Group>
    <Group name="blok_kzit5pdh" visibility="1">
        <Text x="150" y="300" color="#f877dc" size="55" width="800" multiLine="true" text="先生、おかえり！待ってたよ！" align="left" alignV="top" rotation="0" pivotX="0" pivotY="0" scaleX="1" scaleY="1"/>
    </Group>
    <Group name="blok_oxmwcwvq" visibility="1">
        <Var name="w_hziy56" type="number" expression="520"/>
        <Var name="h_hziy56" type="number" expression="357"/>
        <Var name="scaleX_hziy56" type="number" expression="max(0,540/#w_hziy56)" const="true"/>
        <Var name="scaleY_hziy56" type="number" expression="max(0,450/#h_hziy56)" const="true"/>
        <Group x="550" y="450" alpha="255" rotation="0" scaleX="#scaleX_hziy56" scaleY="#scaleY_hziy56">
            <!-- maml-render 字体跟手机有差别 -->
            <Button w="540-50*#scaleX_hziy56" h="450" align="center"/>
            <DateTime name="time1_hziy56" y="0" align="center" size="210" color="#fffb6ff7" formatExp="ifelse(#time_format,'HH:mm','hh:mm')" fontFamily="mitype-normal"/>
            <DateTime y="240" align="center" size="40" color="#fffb6ff7" formatExp="ifelse(#time_format,'M月d日 EEEE','M月d日 Eaa')" bold="true"/>
            <DateTime y="305" align="center" size="40" color="#fffb6ff7" format="YY年,N月e" bold="true"/>
        </Group>
    </Group>
    <Group name="blok_4ngtrkrs" visibility="1" x="0" y="0" rotation="0">
        <FramerateController name="framerate_ani_hfew9b" initPause="true" loop="false">
            <ControlPoint frameRate="60" time="0"/>
            <ControlPoint frameRate="60" time="2000"/>
            <ControlPoint frameRate="30" time="2001"/>
        </FramerateController>
        <Button x="0" y="0" w="1080" h="#screen_height">
            <Triggers>
                <Trigger action="down,up,cancel">
                    <AnimationCommand target="framerate_ani_hfew9b" command="play"/>
                </Trigger>
                <Trigger action="move">
                    <AnimationCommand target="framerate_ani_hfew9b" command="play" condition="#frame_rate{55"/>
                </Trigger>
            </Triggers>
        </Button>
        <Rectangle x="0" y="0" w="1080" h="#screen_height" fillColor="#000000" alpha="#move_x_hfew9b*100/1080"/>
        <!-- 快捷功能图标 -->
        <Group x="#move_x_hfew9b" y="#screen_height-100" alpha="178+#move_y_hfew9b*5">
            <Image name="left_icon_hfew9b" x="100" y="0" align="center" alignV="center" src="defaultScreen_src/4ngtrkrs_left_icon.png" centerX="#left_icon_hfew9b.bmp_width/2" centerY="#left_icon_hfew9b.bmp_height/2" scale="(1-#move_y_hfew9b/300)"/>
            <Image name="right_icon_hfew9b" x="1080-100" y="0" align="center" alignV="center" src="defaultScreen_src/4ngtrkrs_right_icon.png" centerX="#right_icon_hfew9b.bmp_width/2" centerY="#right_icon_hfew9b.bmp_height/2" scale="1/((1-#__pluginWallpaper_move/60)*(1-#move_y_hfew9b/300))"/>
        </Group>
        <!-- 右相机页 -->
        <Rectangle x="1080+#move_x_hfew9b" w="1080" h="#screen_height" fillColor="#000000" visibility="#move_x_hfew9b{0"/>
        <Image x="1080+#move_x_hfew9b" y="#screen_height+(140*(#screen_height==1920))" alignV="bottom" src="defaultScreen_src/cam_bg_0.png" visibility="#move_x_hfew9b{0"/>
        <!-- 左右滑屏按钮 -->
        <Group visibility="#move_y_hfew9b==0**#notice_down==0">
            <!-- 左右移动变量，预留50像素判断是向左右滑动还是上下滑动，当滑动距离超过50时#move_x变量才不为0-->
            <Var name="move_x_hfew9b" expression="max(min(ifelse(#touch_x}#touch_begin_x,max(#touch_move_hfew9b-50,0),min(#touch_move_hfew9b+50,0))+#move_go_hfew9b,1080),-1080)"/>
            <Var name="move_go_hfew9b">
                <VariableAnimation initPause="true" loop="false">
                    <Item value="#move_x1_hfew9b" time="0" easeType="QuadEaseOut"/>
                    <Item value="#move_x2_hfew9b" time="260"/>
                </VariableAnimation>
            </Var>
            <Button w="1080" h="#screen_height">
                <Triggers>
                    <Trigger action="move">
                        <VariableCommand name="touch_move_hfew9b" expression="#touch_x-#touch_begin_x"/>
                    </Trigger>
                    <!-- 判断回弹 -->
                    <Trigger action="up,cancel">
                        <VariableCommand name="move_x1_hfew9b" expression="#move_x2_hfew9b+ifelse(#touch_x}#touch_begin_x,max(#touch_move_hfew9b-50,0),min(#touch_move_hfew9b+50,0))"/>
                        <VariableCommand name="move_x2_hfew9b" expression="ifelse(abs(#touch_x-#touch_begin_x)}300,max(min(#move_x2_hfew9b+ifelse(#touch_x}#touch_begin_x,1,-1)*1080,1080),-1080),#move_x2_hfew9b)"/>
                        <AnimationCommand target="move_go_hfew9b" command="play"/>
                        <VariableCommand name="touch_move_hfew9b" expression="0"/>
                    </Trigger>
                    <Trigger action="up,cancel" condition="abs(#touch_x-#touch_begin_x)}300">
                        <!-- 不解锁进入相机界面 -->
                        <IntentCommand action="android.intent.action.MAIN" package="com.android.camera" class="com.android.camera.Camera" condition="#touch_begin_x-#touch_x}300**#move_x_hfew9b{0">
                            <Extra name="ShowCameraWhenLocked" type="boolean" expression="1"/>
                            <Extra name="StartActivityWhenLocked" type="boolean" expression="1"/>
                        </IntentCommand>
                    </Trigger>
                </Triggers>
            </Button>
        </Group>
        <!-- 解锁 -->
        <Group visibility="#move_x_hfew9b==0**#notice_down==0">
            <!-- 上划位移变量，预留50像素判断是向左右滑动还是向上滑动，当向上滑动距离超过50时#move_y变量才不为0 -->
            <Var name="move_y_hfew9b" expression="min(#touch_y-#touch_begin_y+50,0)*#unlock_flag_hfew9b+#unlock_back_hfew9b"/>
            <!-- 上划解锁未到解锁区域时返回弹动动效 -->
            <Var name="unlock_back_hfew9b">
                <VariableAnimation initPause="true" loop="false">
                    <Item value="0" time="0"/>
                    <Item value="min(#touch_y-#touch_begin_y+50,0)" time="100" easeType="CubicEaseOut"/>
                    <Item value="0" time="400"/>
                    <Item value="min(#touch_y-#touch_begin_y+50,0)" time="500" easeType="CubicEaseOut"/>
                    <Item value="-#screen_height" time="1499"/>
                    <Item value="0" time="1500"/>
                </VariableAnimation>
            </Var>
            <Button w="1080" h="#screen_height">
                <Triggers>
                    <Trigger action="down">
                        <VariableCommand name="unlock_flag_hfew9b" expression="1"/>
                    </Trigger>
                    <Trigger action="up,cancel">
                        <VariableCommand name="unlock_flag_hfew9b" expression="0"/>
                        <AnimationCommand target="unlock_back_hfew9b" command="play(ifelse((#touch_begin_y-#touch_y){=300,100,500),ifelse((#touch_begin_y-#touch_y){=300,400,1500))"/>
                        <ExternCommand command="unlock" condition="ge(#touch_begin_y-#touch_y,300)"/>
                    </Trigger>
                </Triggers>
            </Button>
        </Group>
        <Group x="#move_x_hfew9b" y="#move_y_hfew9b" name="container_hfew9b">
            <Group name="负一屏_hfew9b" x="-1080" visibility="#move_x_hfew9b}0">
                <Group name="blok_eqqlu15n" visibility="1">
                    <Button/>
                </Group>
            </Group>
            <Group name="正一屏_hfew9b" x="0" visibility="#move_x_hfew9b{1080**#move_x_hfew9b}-1080">
                <Group name="blok_en90ei0g" visibility="1">
                    <Button/>
                </Group>
            </Group>
        </Group>
    </Group>
    <Group name="blok_ycodbcql" visibility="1">
