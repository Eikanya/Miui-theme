                <Button x="0" y="0" w="1024" h="(#h_ri7mss+20)*#scaleY_ri7mss" alignChildren="true">
                    <Normal>
                        <Image src="notification_src/vzhc7e8c_notice_bg.png" alpha="255" w="#w_ri7mss" h="#h_ri7mss"/>
                    </Normal>
                    <Pressed>
