                <!-- 播放	暂停	按钮图片 -->
                <Button x="950/2+(167+(950-950)/4)" y="198" w="130" h="130" align="center" alignChildren="true" scale="0.5+ifelse(#resumeMusicButtonVi_0qgrgr==1,1,#albumCoverAni_0qgrgr{0.5,1-2*#albumCoverAni_0qgrgr,#albumCoverAni_0qgrgr)/2" alpha="ifelse(#resumeMusicButtonVi_0qgrgr==1,255,#albumCoverAni_0qgrgr{0.5,255-510*#albumCoverAni_0qgrgr,255*#albumCoverAni_0qgrgr)">
                    <Normal>
                        <Image x="130/2" y="130/2" align="center" alignV="center" srcExp="'musicMayEleven_src/'+ifelse(#resumeMusicButtonVi_0qgrgr==1||#albumCoverAni_0qgrgr}0.5,'pause','play')+'.png'" alpha="128" pivotX="65" pivotY="65"/>
                    </Normal>
                    <Pressed>
                        <Image x="130/2" y="130/2" align="center" alignV="center" srcExp="'musicMayEleven_src/'+ifelse(#resumeMusicButtonVi_0qgrgr==1||#albumCoverAni_0qgrgr}0.5,'pause','play')+'.png'" alpha="100" pivotX="65" pivotY="65"/>
                    </Pressed>
                    <Triggers>
                        <Trigger action="up">
                            <VariableCommand name="buttonVi_0qgrgr" expression="0"/>
                            <AnimationCommand target="albumCoverAni_0qgrgr" command="play(300,600)" condition="#albumCoverAni_0qgrgr==0**#music_control_0qgrgr.music_state==0"/>
                            <AnimationCommand target="albumCoverAni_0qgrgr" command="play(600,900)" condition="#albumCoverAni_0qgrgr==1**#music_control_0qgrgr.music_state==1"/>
                            <AnimationCommand target="buttonDownAni_0qgrgr" command="play" condition="#buttonDownAni_0qgrgr==0"/>
                        </Trigger>
                    </Triggers>
                </Button>
                <!-- 播放 -->
                <Button name="music_play" x="950/2+(167+(950-950)/4)" y="198" w="130" h="130" align="center"/>
                <!-- 暂停 -->
                <Button name="music_pause" x="950/2+(167+(950-950)/4)" y="198" w="130" h="130" align="center"/>
            </MusicControl>
        </Group>
    </Group>
</Lockscreen>